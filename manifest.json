{
  "name": "Bookmarks",
  "version": "1.0",
  "description": "Simple and cool bookmarks manager",
  "permissions": ["declarativeContent", "storage"],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "img/bookmark.png",
      "32": "img/bookmark32.png",
      "48": "img/bookmark48.png",
      "64": "img/bookmark64.png",
      "128": "img/bookmark128.png"
    },
    "default_popup": "bookmarks.html"
  },
  "icons": {
    "16": "img/bookmark.png",
    "32": "img/bookmark32.png",
    "48": "img/bookmark48.png",
    "64": "img/bookmark64.png",
    "128": "img/bookmark128.png"
  }
}